{
  "project_name": "hyp3-plugin",
  "project_title": "{{cookiecutter.project_name|replace('-', ' ')|replace('hyp3', 'HyP3')|title}}",
  "package_name": "{{cookiecutter.project_name|lower|replace('-', '_')}}",
  "process_name": "{{cookiecutter.package_name|replace('hyp3', 'proc')}}",
  "process_type": "{{cookiecutter.package_name|replace('hyp3', '')|trim(chars='_')}}",
  "short_description": "HyP3 plugin for {{cookiecutter.process_type}} processing",
  "public_url": "https://scm.asf.alaska.edu/hyp3/{{cookiecutter.project_name}}",
  "copyright_year": "{% now 'utc', '%Y' %}",
  "_extensions": ["jinja2_time.TimeExtension"]
}